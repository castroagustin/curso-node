<h1 class="mb-3 mt-5">Historial</h1>
<% if(productosArr.length >= 1) { %>
<table class="table">
    <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Titulo</th>
        <th scope="col">Precio</th>
        <th scope="col">Link a imagen</th>
      </tr>
    </thead>
    <tbody>
    <% productosArr.forEach(data => { %>
      <tr>
        <td><%= data.id %></td>
        <td><%= data.title %></td>
        <td><%= data.price %></td>
        <td><%= data.thumbnail %></td>
      </tr>
      <%})%>
    </tbody>
  </table>
<%} else {%>
    <div class="alert alert-danger">
        No se encontraron productos
      </div>
<%}%>
<a class="btn btn-secondary mt-3" href="/">Ir al formulario</a>