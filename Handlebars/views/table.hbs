<h1 class="mb-3 mt-5">Productos</h1>
{{#if isNotEmpty}}
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Titulo</th>
            <th scope="col">Precio</th>
            <th scope="col">Imagen</th>
        </tr>
    </thead>
    <tbody>
        {{#each productosArr}}
        <tr>
            <td>
                {{this.id}}
            </td>
            <td>
                {{this.title}}
            </td>
            <td>
                {{this.price}}
            </td>
            <td>
                {{this.thumbnail}}
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{else}}
<div class="alert alert-danger">
    No se encontraron productos
</div>
{{/if}}
<a class="btn btn-secondary mt-3" href="/">Ir al formulario</a>