<head>
    <link rel="stylesheet" href="../style.css">
</head>
{{#if productos.length}}
<h3 class="mb-3 mt-5">Productos</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Titulo</th>
                <th scope="col">Precio</th>
                <th scope="col">Imagen</th>
            </tr>
        </thead>
        <tbody>
            {{#each productos}}
            <tr>
                <td>
                    {{this.id}}
                </td>
                <td>
                    {{this.title}}
                </td>
                <td>
                    {{this.price}}
                </td>
                <td>
                    <img src={{this.thumbnail}} class="prodsTable__img">
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    {{/if}}